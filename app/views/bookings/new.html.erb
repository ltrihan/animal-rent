<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8 col-md-offset-2">
      <h1> Rent me baby !!!</h1>
  
        <%= simple_form_for([@animal, @booking]) do |f| %>
          <div class="form-group">
            <%= f.input :start_at, as: :date, html5: true, class: 'form-control' %>
          </div>

          <div class="form-group">
            <%= f.input :end_at, as: :date, html5: true, class: 'form-control' %>
          </div>

          <%= f.submit class: 'btn btn-primary' %>
        <% end %>
      </div>
   </div> 
</div>
