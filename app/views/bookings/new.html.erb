<div class="container">
  <div class="row">
    <div class="col-md-8 col-md-offset-2">
      <h1> Rent me baby !!!</h1>
  
        <%= simple_form_for([@animal, @booking]) do |f| %>
          <div class="form-group">
            <%= f.label :start_at %>
            <%= f.text_field :start_at, class: 'form-control' %>
          </div>

          <div class="form-group">
            <%= f.label :end_at %>
            <%= f.text_field :end_at, class: 'form-control' %>
          </div>

          <%= f.submit class: 'btn btn-primary' %>
        <% end %>
      </div>
   </div> 
</div>
